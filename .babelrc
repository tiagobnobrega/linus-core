{
  "presets": [
    ["env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ],
          "node": "8.0.0"
        },
        "shippedProposals": true
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    "transform-class-properties"
  ]
}