{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },

  "globals": {
    "process": false,
    "require": false
  },

  "extends": ["airbnb-base","prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules":{
    "class-methods-use-this":0,
//    "eslint no-underscore-dangle": 0 globally disable "_" properties
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}